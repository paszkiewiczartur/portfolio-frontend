{
  "name": "my-second-app",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start-proxy": "ng serve --proxy-config proxy.conf.json",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "e2e:pl": "ng e2e --aot --i18nFile=src/locale/messages.pl.xlf --i18nFormat=xlf --locale=pl",
    "start:pl": "ng serve --i18nFile=src/locale/messages.pl.xlf --i18nFormat=xlf --locale=pl",
    "start:en": "ng serve --i18nFile=src/locale/messages.xlf --i18nFormat=xlf --locale=en",
    "build:pl": "ng build --prod --no-aot --i18nFile=src/locale/messages.pl.xlf --i18nFormat=xlf --locale=pl",
    "build:en": "ng build --prod --no-aot --i18nFile=src/locale/messages.xlf --i18nFormat=xlf --locale=en --base-href=/en/",
    "extract": "ng xi18n --outputPath=src/locale"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^5.2.9",
    "@angular/cdk": "^5.1.1",
    "@angular/cli": "^1.7.3",
    "@angular/common": "^5.2.9",
    "@angular/compiler": "^5.2.9",
    "@angular/compiler-cli": "^5.2.9",
    "@angular/core": "^5.2.9",
    "@angular/forms": "^5.2.9",
    "@angular/http": "^5.2.9",
    "@angular/material": "^5.1.1",
    "@angular/platform-browser": "^5.2.9",
    "@angular/platform-browser-dynamic": "^5.2.9",
    "@angular/platform-server": "^5.2.9",
    "@angular/router": "^5.2.9",
    "@angular/upgrade": "^5.2.9",
    "@ng-bootstrap/ng-bootstrap": "^1.0.1",
    "@ngrx/effects": "^5.2.0",
    "@ngrx/router-store": "^5.2.0",
    "@ngrx/store": "^5.2.0",
    "@ngrx/store-devtools": "^5.0.1",
    "bootstrap": "^3.3.7",
    "core-js": "^2.4.1",
    "hammerjs": "^2.0.8",
    "rxjs": "^5.5.2",
    "zone.js": "^0.8.14"
  },
  "devDependencies": {
    "@angular-devkit/core": "^0.2.0",
    "@angular/cli": "^1.7.3",
    "@angular/compiler-cli": "^5.2.4",
    "@angular/language-service": "^5.0.0",
    "@types/jasmine": "2.5.53",
    "@types/jasminewd2": "^2.0.3",
    "@types/node": "~6.0.60",
    "codelyzer": "~3.2.2",
    "jasmine-core": "~2.6.2",
    "jasmine-spec-reporter": "~4.1.0",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "lodash": "^4.16.2",
    "phantomjs-prebuilt": "^2.1.7",
    "protractor": "~5.1.2",
    "ts-node": "~3.0.4",
    "tslint": "~5.7.0",
    "typescript": "~2.4.2"
  }
}
